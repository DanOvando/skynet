  journal = {Biometrics},
  author = {Chapman, D. G. and Robson, D. S.},
  month = sep,
  year = {1960},
  pages = {354-368},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Chapman and Robson - 1960 - The Analysis of a Catch Curve.pdf}
}

@article{Hamilton2010,
