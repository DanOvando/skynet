  month = jun,
  year = {2012},
  keywords = {Bio-economic models,Review,fisher behaviour,fishery modelling,fleet dynamics},
  pages = {216-235},
  file = {/Users/danovando/Reference Library/van Putten et al. - 2012 - Theories and behavioural drivers underlying fleet .pdf}
}

