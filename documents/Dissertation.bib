  number = {6186},
  urldate = {2016-06-03},
  url = {http://science.sciencemag.org/content/344/6186/895},
  journal = {Science},
  author = {Palumbi, Stephen R. and Barshis, Daniel J. and {Traylor-Knowles}, Nikki and Bay, Rachael A.},
  month = may,
  year = {2014},
  pages = {895-898},
  file = {/Users/danovando/SFG Work/General Library/Reference Library/Palumbi et al. - 2014 - Mechanisms of reef coral resistance to future clim.pdf},
  pmid = {24762535}
