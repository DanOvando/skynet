  url = {http://www.sciencedirect.com/science/article/pii/S0165783608003160},
  journal = {Fisheries Research},
  author = {Dowling, Natalie A. and Smith, David C. and Knuckey, Ian and Smith, Anthony D. M. and Domaschenz, Phil and Patterson, Heather M. and Whitelaw, Wade},
  month = dec,
  year = {2008},
  keywords = {Iterative approach,Low-value fisheries,Pragmatic management,Stakeholder engagement,harvest strategy,Data-poor},
  pages = {380-390},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Dowling et al. - 2008 - Developing harvest strategies for low-value and da.pdf}
