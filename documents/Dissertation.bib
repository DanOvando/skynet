  journal = {Biological Conservation},
  author = {{Harmelin-Vivien}, Mireille and Le Diréach, Laurence and {Bayle-Sempere}, Just and Charbonnel, Eric and {García-Charton}, José Antonio and Ody, Denis and {Pérez-Ruzafa}, Angel and Reñones, Olga and {Sánchez-Jerez}, Pablo and Valle, Carlos},
  month = jul,
  year = {2008},
  keywords = {Fisheries,Marine reserves,Fish spillover,Visual census},
  pages = {1829-1839},
  file = {/Users/danovando/Reference Library/Harmelin-Vivien et al. - 2008 - Gradients of abundance and biomass across reserve .pdf}
}

