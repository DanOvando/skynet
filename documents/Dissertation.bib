  month = feb,
  year = {2010},
  keywords = {derived oceanographic distance,dispersal,isolation by distance,pelagic larvae,seascape genetics},
  pages = {rspb20092214},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/White et al. - 2010 - Ocean currents help explain population genetic str.pdf},
  pmid = {20133354}
