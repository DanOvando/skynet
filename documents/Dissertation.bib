  journal = {PLoS One},
  author = {Sulu, Reuben J. and Eriksson, Hampus and Schwarz, Anne-Maree and Andrew, Neil L. and Orirana, Grace and Sukulu, Meshach and Oeta, Janet and Harohau, Daykin and Sibiti, Stephen and Toritela, Andrew and Beare, Douglas},
  month = nov,
  year = {2015},
  keywords = {Agricultural production,Agriculture,Alternatives,Commercial fishing,Fishing,Food,Islands,Medical Sciences,Population,Sanitation,Sciences: Comprehensive Works,fisheries management},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Sulu et al. - 2015 - Livelihoods and Fisheries Governance in a Contempo.pdf}
}

