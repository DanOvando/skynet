  journal = {Coral Reefs},
  author = {Almany, G. R. and Connolly, S. R. and Heath, D. D. and Hogan, J. D. and Jones, G. P. and McCook, L. J. and Mills, M. and Pressey, R. L. and Williamson, D. H.},
  month = mar,
  year = {2009},
  keywords = {Conservation planning,Freshwater \& Marine Ecology,Great Barrier Reef,Oceanography,Resilience,Risk-spreading,larval dispersal,marine protected area},
  pages = {339-351},
  file = {/Users/danovando/SFG Work/General Library/Reference Library/Almany et al. - 2009 - Connectivity, biodiversity conservation and the de.pdf}
