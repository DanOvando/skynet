  keywords = {Marine protected areas,Monitoring,kelp forest ecosystem,response ratios,ecosystem-based management},
  pages = {18272-18277},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Hamilton et al. - 2010 - Incorporating biogeography into evaluations of the.pdf},
  pmid = {20176956}
}

