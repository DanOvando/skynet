  keywords = {Statistics - Methodology},
  file = {/Users/danovando/Papers and Projects/General Library/Reference Library/de Chaisemartin and D'Haultfoeuille - 2015 - Fuzzy Differences-in-Differences.pdf}
}

@article{Brown2014,
