  keywords = {Fisheries,Marine reserves,biodiversity,dispersal,models,conservation},
  pages = {65-70},
  file = {/Users/danovando/Desktop/Bren/SFG Work/General Library/Reference Library/Hastings and Botsford - 2003 - COMPARING DESIGNS OF MARINE RESERVES FOR FISHERIES.pdf;/Users/danovando/Zotero/storage/ZPQSFZ7K/showCitFormats.html}
