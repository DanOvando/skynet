  url = {http://icesjms.oxfordjournals.org/content/early/2016/10/21/icesjms.fsw169},
  journal = {ICES Journal of Marine Science: Journal du Conseil},
  author = {Melnychuk, Michael C. and Clavelle, Tyler and Owashi, Brandon and Strauss, Kent},
  month = oct,
  year = {2016},
  keywords = {Environmental Economics,fish markets,fisheries revenue,landed value,price transmission,seafood trade.},
